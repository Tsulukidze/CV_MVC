@using CV_MVC.Models.Entity
@model List<TBLABOUT>

<div class="resume-section-content">
    @foreach (var item in Model)
    {

        <h1 class="mb-0">
            @item.Name
            <span class="text-primary">@item.Surname</span>
        </h1>
        <div class="subheading mb-5">
            @item.Adress · @item.Phone·
            <a href="https://mail.google.com/" target="_blank">@item.Email</a>
        </div>
        <p class="lead mb-5">@item.Details</p>
        <div class="social-icons">
            <a class="social-icon" href="https://www.linkedin.com/in/temur-tsulukidze-64a9ab1b1/" target="_blank"><i class="fab fa-linkedin-in"></i></a>
            <a class="social-icon" href="https://github.com/Tsulukidze" target="_blank"><i class="fab fa-github"></i></a>
            <a class="social-icon" href="https://www.youtube.com/channel/UCnMq8sWfIXAwVmXCEV5es7Q" target="_blank"><i class="fab fa-youtube"></i></a>
            <a class="social-icon" href="https://www.facebook.com/tsulukidze8/" target="_blank"><i class="fab fa-facebook-f"></i></a>
        </div>
    }
</div>
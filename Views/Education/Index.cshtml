@using CV_MVC.Models.Entity
@model List<TBLEDUCATION>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />

<h2>List of Education</h2>
<br />
<table class="table table-hover">
    <thead>
        <tr>
            <th style="width: 10px">#</th>
            <th>Header</th>
            <th>SubHeader</th>
            <th>SubHeader2</th>
            <th>Date</th>
            <th>GPA</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>@item.ID</th>
                <th>@item.Header</th>
                <th>@item.SubHeader1</th>
                <th>@item.SubHeader2</th>
                <th>@item.Date</th>
                <th>@item.GPA</th>
                <th><a href="/Education/EditEducation/@item.ID" class="btn btn-outline-success"> Edit </a></th>
                <th><a href="/Education/DeleteEducation/@item.ID" class="btn btn-outline-danger"> Delete </a></th>
                <th></th>
            </tr>
        }
    </tbody>
</table>
<a href="/Education/AddEducation" class="btn btn-outline-primary">Add new Education</a>




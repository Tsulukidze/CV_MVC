@using CV_MVC.Models.Entity
@model List<TBLEXPERIENCE>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />

<h2>List of Experiences</h2>
<br />
<table class="table table-hover">
    <thead>
        <tr>
            <th style="width: 10px">#</th>
            <th>Header</th>
            <th>SubHeader</th>
            <th>Date</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>@item.ID</th>
                <th>@item.Header</th>
                <th>@item.SubHeader</th>
                <th>@item.Date</th>
                <th><a href="/Experience/GetExperience/@item.ID" class="btn btn-outline-success"> Edit </a></th>
                <th><a href="/Experience/DeleteExperience/@item.ID" class="btn btn-outline-danger"> Delete </a></th>

                <th></th>
            </tr>
        }
    </tbody>
</table>
<a href="/Experience/AddExperience" class="btn btn-outline-primary">Add new experience</a>

